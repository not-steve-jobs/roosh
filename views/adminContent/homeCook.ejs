
<%- include ('admin_profile_header') -%>

<link rel="stylesheet" href="/css/datatable/datatable.min.css" />

<style>
    body {
        font-size: 12pt;
        font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen,
        Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
    }
    table td {
        font-size: 14px;
    }
</style>
<h1 style="padding-bottom: 50px">HomeCook</h1>

<table data-replace="jtable" id="example" aria-label="JS Datatable" data-locale="en" data-search="true">
    <thead>
    <tr>
        <th>name</th>
        <th>surname</th>
        <th>address</th>
        <th>phone</th>
        <th>email</th>
        <th>description</th>
    </tr>
    </thead>
    <tbody>
    <%data.forEach((item)=>{%>
    <tr>
        <td><%=item.name%></td>
        <td><%=item.surname%></td>
        <td><%-item.address%></td>
        <td><%=item.phone%></td>
        <td><%=item.email%></td>
        <td><%=item.desc%></td>
    </tr>
    <%})%>
</table>
<script src="/js/datatable/datatable.min.js"></script>


<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<!--<script src="js/jquery.js"></script>-->

<%- include ('admin_profile_footer') -%>