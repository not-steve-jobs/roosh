<%- include ('admin_profile_header') -%>

<link rel="stylesheet" href="/css/datatable/datatable.min.css" />

<style>
    body {
        font-size: 12pt;
        font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen,
        Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
    }
</style>
<h1 style="padding-bottom: 50px">Adminblog</h1>

<table data-replace="jtable" id="example" aria-label="JS Datatable" data-locale="en" data-search="true">
    <thead>
    <tr>
        <th style="width: 15%">URL</th>
        <th style="width: 20%">Title</th>
        <th>Description</th>
    </tr>
    </thead>
    <tbody>
    <%data.forEach((item)=>{%>
        <tr>
            <td><a href="/admin_profile/thisBlog/<%=item.url%>">url: /<%=item.url%></a></td>
            <td><%=item.title%></td>
            <td><div style="height: 150px; overflow: hidden"><%-item.desc%></div></td>
        </tr>
    <%})%>
</table>
<script src="/js/datatable/datatable.min.js"></script>


<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<!--<script src="js/jquery.js"></script>-->

<%- include ('admin_profile_footer') -%>